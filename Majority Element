class Solution {
    public int majorityElement(int[] a) {

        int cnt=0, n = a.length;
        int el = 0;
       for(int i = 0;i<n;i++){
        if(cnt==0){
            cnt++;
            el = a[i];
        }
        else if(a[i]==el){
       cnt++;
        }
        else{
            cnt--;
        }
       }
       return el;
    }
    
}
