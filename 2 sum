class Solution {
    public int[] twoSum(int[] a, int t) {
        int n = a.length;
      
       HashMap<Integer,Integer> mp = new HashMap<>();
        for (int i = 0; i < n; i++) {
            int ans = t-a[i];
            if(mp.containsKey(ans)){
                return new int []{i,mp.get(ans)};
            }
            mp.put(a[i],i);
        }
        return new int[] {};  
    }
}
