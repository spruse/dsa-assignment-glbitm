/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    private static void pre(TreeNode root ,List <Integer> ls){

        if(root == null) return ;

        ls.add(root.val);
        pre(root.left ,ls);
        pre(root.right , ls);

    }
    public List<Integer> preorderTraversal(TreeNode root) {
        List <Integer> ls = new ArrayList<>();

        // iterative approach

        Stack<TreeNode> ls = new Stack<>();

        st.push(root);

        while(!st.isEmpty()){
            root  = st.pop();

            ls.push(root.data);
            if(root.right != null)
            st.push(root.right);
            if(root.left !=null)
            st.push(root.left);

        }
        return ls;        
    }
}
