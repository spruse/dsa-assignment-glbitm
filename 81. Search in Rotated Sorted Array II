class Solution {
    public boolean search(int[] a, int target) {
        int n = a.length;
        int low = 0;
        int high = n-1;
        while(low<=high){
            int mid = (high+low)/2;
            if(a[mid]==target) return true;
            if (a[low]==a[mid]){
                while(a[low]==a[mid]){
                    low++;
                }
            }
          if (a[low]<a[mid]){
            if(target>a[low]&&target<a[mid]){
                high = mid-1;

            }
            else{
                low=mid+1;
            }
          }

          else {
            if(target>a[mid]&&target <a[high]){
                low = mid+1;
            }
            else{
                high = mid-1;
            } 
          }
          
        }
        return false;
    }
}
