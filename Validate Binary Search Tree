/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    
    public static boolean who(TreeNode root , long low , long high){

        if(root == null ) return true;

        if(root.val >= high || root.val <= low)// i.e checking for if its in range
        return false;

        return (who(root.left , low , root.val)  &&
                who(root.right , root.val , high));
      
    }
    public boolean isValidBST(TreeNode root) {
       return who(root , Long.MIN_VALUE , Long.MAX_VALUE);
        
    }
}
